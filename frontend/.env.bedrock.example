# Frontend environment configuration for AgentCore Runtime
# Copy to frontend/.env.local and fill in your values after deployment

# Region where AgentCore Runtime is deployed
VITE_REGION=us-east-1

# AgentCore Runtime ARN (from CloudFormation outputs after deployment)
VITE_AGENT_RUNTIME_ARN=YOUR_RUNTIME_ARN_HERE

# Cognito User Pool ID (from CloudFormation outputs)
VITE_USER_POOL_ID=YOUR_USER_POOL_ID_HERE

# Cognito User Pool Client ID (from CloudFormation outputs)
VITE_USER_POOL_CLIENT_ID=YOUR_USER_POOL_CLIENT_ID_HERE

# Local dev mode (set to 'true' when running dev-local.ps1/sh)
VITE_LOCAL_DEV=false
